@{
}



<table border="1">
    <tr>
        <th>    Company Name  </th>
        <th>    Ticker  </th>
        <th>    Market Cap  </th>
    </tr>
@foreach (var s in ViewBag.Stocks)
{
    <tr>
        <td> @s.CompanyName </td>
        <td> @s.Ticker </td>
        <td> @s.MarketCap </td>
    </tr>
}

</table>